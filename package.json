{
  "name": "typed-web-workers",
  "version": "3.0.0-next",
  "description": "Create web workers with type safety",
  "main": "lib/index",
  "typings": "lib/index",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "rimraf lib/ && tsc -p . && npm run build:umd",
    "build:umd": "tsc -p . --module umd --outDir lib/umd",
    "test": "karma start karma.conf.js",
    "lint": "prettier \"./{src,test}/**.ts\" --write"
  },
  "keywords": [
    "web",
    "worker",
    "typescript"
  ],
  "author": "anderscan",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "2.8.8",
    "codecov": "3.1.0",
    "jasmine": "3.2.0",
    "jasmine-core": "3.2.1",
    "karma": "3.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.2",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "1.1.2",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "3.0.4",
    "prettier": "1.14.3",
    "rimraf": "^2.6.2",
    "typescript": "3.1.1"
  },
  "repository": "AndersCan/typed-web-workers"
}
