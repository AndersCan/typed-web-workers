    "jasmine-core": "3.5.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "2.0.1",
    "karma-firefox-launcher": "1.2.0",
    "karma-jasmine": "2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-typescript": "4.1.1",
    "prettier": "1.19.1",
    "rimraf": "^3.0.0",
    "typescript": "3.7.2"
